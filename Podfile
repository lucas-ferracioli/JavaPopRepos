# Uncomment the next line to define a global platform for your project
platform :ios, '13.5'

target 'JavaPopRepos' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JavaPopRepos
  pod 'SnapKit', '~> 5.0.0'
  pod 'Alamofire', '~> 5.2'

  target 'JavaPopReposTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'Nimble-Snapshots'
  end

end
